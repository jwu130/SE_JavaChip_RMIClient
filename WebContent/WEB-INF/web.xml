<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Assignment3Servlet</display-name>

	<servlet>
		<servlet-name>ServerFiles</servlet-name>
		<servlet-class>com.javachip.ServerFiles</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>ServerFiles</servlet-name>
		<url-pattern>/serverFiles</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ConfirmFile</servlet-name>
		<servlet-class>com.javachip.ConfirmFile</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>ConfirmFile</servlet-name>
		<url-pattern>/confirmFile/*</url-pattern>
	</servlet-mapping>

